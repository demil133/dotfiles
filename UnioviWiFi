CONNECTION='wireless'
INTERFACE=wlan0
SECURITY='wpa-configsection'
ESSID='BondStudents'
IP='dhcp'
CONFIGSECTION='
    ssid="<Wifi SSID goes here>"
    key_mgmt=WPA-EAP
    eap=PEAP
    group=CCMP
    pairwise=CCMP
    identity="<student ID goes here>"
    password="<password goes here>"
    priority=1
    ca_path="/etc/ssl/certs"
    phase2="auth=MSCHAPV2"
'
